    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "pythonArgs": ["-Xfrozen_modules=off"]  //, "PYDEVD_DISABLE_FILE_VALIDATION=1"
        },
        {
            "name": "Python: Test Condor Worker",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "results_test.csv",  //output-file
                "ite_afm",  //job_type
                "2",  //req_mem_gb
                "0",  //seed
                "1",  //method
                "2",  //D
                "2",  //N
                "1",  //chi
                "[seed,D,N,chi,iterations,num_tensors,energy,exec_time]"  //result_keys
            ]
        },
        {
            "name": "Python: Test per_dim_sender",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "6", "2"
            ]
        }
    ]
}